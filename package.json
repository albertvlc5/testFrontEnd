{
  "name": "atwork",
  "version": "0.0.1",
  "main": "module.tsx",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --testPathPattern=\"/*.spec.*\" --verbose --maxWorkers=6",
    "test:coverage": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --coverage --maxWorkers=6",
    "test:upload:coverage": "yarn codecov",
    "lint": "npx yarn run lint:js && npx yarn run lint:ts",
    "lint:ts": "eslint -c ./.eslintrc.js . --ext .tsx,.ts",
    "lint:js": "eslint -c ./.eslintrc.js . --ext .js,.jsx",
    "lint:fix": "npx yarn run lint:ts --fix && npx yarn run lint:js --fix",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "check:dependencies": "./node_modules/.bin/dependency-check .",
    "check:ts": "./node_modules/.bin/check-typescript",
    "run-on-core": "core-updater mobile-store"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npx yarn run check:ts --details && npx yarn lint"
    }
  },
  "dependencies": {
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@creditas/mobile-core-api": "1.0.1",
    "@react-native-community/async-storage": "1.7.0",
    "@react-native-community/eslint-config": "1.1.0",
    "@react-native-community/masked-view": "0.1.1",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "24.0.24",
    "@types/react-native": "0.62.4",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "25.1.0",
    "codecov": "^3.6.1",
    "cross-env": "^7.0.2",
    "eslint": "6.5.1",
    "eslint-plugin-prettier": "^3.1.3",
    "full-icu": "1.3.1",
    "husky": "4.3.0",
    "intl": "^1.2.5",
    "jest": "24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "0.59.0",
    "mobile-core-api-mock": "git+ssh://git@github.com:Creditas/mobile-core-api-mock.git#0.0.45",
    "mobile-dev-tools": "git+ssh://git@github.com:Creditas/mobile-dev-tools.git#0.0.35",
    "mobile-styles": "git+ssh://git@github.com:Creditas/mobile-styles.git",
    "prettier": "2.0.4",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-gesture-handler": "1.5.3",
    "react-native-linear-gradient": "2.5.6",
    "react-native-reanimated": "1.7.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-share": "2.0.0",
    "react-native-svg": "9.13.6",
    "react-navigation": "4.1.1",
    "react-navigation-stack": "2.8.4",
    "react-test-renderer": "16.13.1",
    "rn-fetch-blob": "0.12.0",
    "ts-jest": "24.2.0",
    "typescript": "3.8.3"
  }
}
